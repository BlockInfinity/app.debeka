{
  "name": "inspinia-reactjs",
  "version": "2.7.0",
  "scripts": {
    "start": "npm-run-all --parallel start:server start:testrpc start:build",
    "start:server": "DEBUG='express:*' nodeUrl='http://localhost:8545' nodemon --ignore './server/contractData/' server.js",
    "start:build": "webpack --watch --config webpack.config.js",
    "start:testrpc": "testrpc -m 'snake ozone true betray silver fault slice destroy file sweet comic naive' --accounts 100 --gasLimit 0x7A1200",
    "build": "BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js"
  },
  "keywords": [
    "inspinia",
    "react",
    "reactjs",
    "webpack"
  ],
  "author": "webapplayers",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "path": "^0.12.7",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "animate.css": "^3.1.1",
    "assert": "^1.4.1",
    "bootstrap": "^3.3.7",
    "chai": "^4.1.2",
    "co": "^4.6.0",
    "dom-tools": "^0.1.4",
    "ethereumjs-util": "^5.1.2",
    "font-awesome": "^4.7.0",
    "history": "^3.0.0",
    "jquery": "^3.1.1",
    "jquery-slimscroll": "^1.3.6",
    "metismenu": "^2.5.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.6",
    "react-pace": "^1.0.0",
    "react-router": "^2.7.0",
    "socket.io": "^2.0.4",
    "solc": "^0.4.18",
    "tedious": "^2.0.1",
    "web3": "^0.19.0"
  }
